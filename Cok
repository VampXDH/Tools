from time import sleep
from DrissionPage import ChromiumPage

# Inisialisasi ChromiumPage
p = ChromiumPage()

# Membuka halaman yang diinginkan
p.get('https://panel.f-network.my.id/')

# Mengambil iframe yang sesuai
i = p.get_frame('@src^="https://challenges.cloudflare.com/cdn-cgi"')

# Menemukan elemen dengan class `.mark`
e = i('.mark')

# Menunggu beberapa detik sebelum klik
sleep(3)

# Melakukan klik pada elemen
e.click()
